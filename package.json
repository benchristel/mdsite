{
  "name": "@benchristel/mdsite",
  "version": "0.1.1",
  "description": "A modern generator for old-school static sites",
  "main": "dist/main.js",
  "repository": "git@github.com:benchristel/mdsite",
  "author": "Ben Christel <ben.christel@gmail.com>",
  "license": "MIT",
  "private": false,
  "bin": "./dist/main.js",
  "devDependencies": {
    "@types/marked": "^4.0.8",
    "@types/node": "^18.15.11",
    "husky": "^8.0.3",
    "prettier": "^2.8.7",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@benchristel/taste": "0.5.0",
    "@types/yargs-parser": "^21.0.0",
    "cheerio": "^1.0.0-rc.12",
    "highlight.js": "^11.8.0",
    "marked": "^4.3.0",
    "marked-highlight": "^2.0.1",
    "yargs-parser": "^21.1.1"
  },
  "scripts": {
    "docs": "./mdsite -i docs-src",
